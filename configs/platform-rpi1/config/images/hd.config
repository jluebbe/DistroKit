image @IMAGE@ {
	hdimage {
		align = 1M
		partition-table-type = hybrid
	}
	partition boot-vfat {
		image = boot.vfat
		partition-type-uuid = F
		partition-type = 0xc
		bootable = true
	}
	partition root {
		image = root.ext2
		partition-type-uuid = 69dad710-2ce4-4e3c-b16c-21a1d49abed3 # root-arm
	}
}
